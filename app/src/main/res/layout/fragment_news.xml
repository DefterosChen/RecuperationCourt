<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".common.view.fragment.consult.NewsFragment">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_0.5_dip"
        android:background="@color/gainsboro" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/frag_item_listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/dimen_8_dip" />

    <ImageView
        android:id="@+id/img_itemtwofrag_load"
        android:src="@drawable/loading_anim"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/dimen_20_dip"
        android:layout_width="@dimen/dimen_50_dip"
        android:layout_height="@dimen/dimen_70_dip" />


</FrameLayout>